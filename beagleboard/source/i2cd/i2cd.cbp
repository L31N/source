<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="i2cd" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="ARM">
				<Option output="bin/ARM/i2cd" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/ARM/" />
				<Option type="1" />
				<Option compiler="armelfgcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="/bbusr/include" />
		</Compiler>
		<Linker>
			<Add option="-lmpu6050" />
			<Add option="-lbbvector" />
			<Add option="-lbbdebug" />
			<Add option="-lbbipc" />
			<Add directory="/bbusr/lib" />
		</Linker>
		<Unit filename="main.cpp" />
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
